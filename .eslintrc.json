{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/destructuring-assignment": 0,
    "no-undef": 0,
    "react/jsx-filename-extension": 0,
    "react/button-has-type": 0,
    "consistent-return": 0,
    "no-alert": 0,
    "react/no-multi-comp": 0
  }
}
